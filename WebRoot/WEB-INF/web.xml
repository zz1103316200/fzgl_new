    <url-pattern>/fzgl.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>qbfzServlet</servlet-name>
    <url-pattern>/qbfz.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>qbkzServlet</servlet-name>
