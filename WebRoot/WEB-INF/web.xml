      <param-name>servicePath</param-name>
      <param-value>D:\\myeclipsePro\\highService\\Surface.jar</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>openServicePackServlet</servlet-name>
    <servlet-class>cetc.openServicePackServlet</servlet-class>
    <init-param><!--初始化参数,可以在init()中使用ServletConfig().getInitParamenter("name")取得-->
      <param-name>servicePackPath</param-name>
      <param-value>D:\\myeclipsePro\\ServicePackaging\\CS\\ServicePackage_fat.jar</param-value>
    </init-param>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
